{
  "name": "{{projectName}}-frontend",
  "version": "1.0.0",
  "type": "module",
  "author": "Michal Rakus",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "buildAndPackLib": "pnpm --prefix ../../chilibase/frontend run buildAndPack",
    "installLib": "pnpm install ../../chilibase/frontend/chilibase-frontend-0.1.2.tgz",
    "bl": "pnpm run buildAndPackLib && pnpm run installLib",
    "sl": "pnpm run bl && pnpm run dev",
    "preinstall": "npx only-allow pnpm"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@chilibase/frontend": "0.1.2",
    "axios": "^1.8.4",
    "lodash": "^4.17.21",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primereact": "10.9.4",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.6.2",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/lodash": "^4.17.16",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "@types/react-transition-group": "^4.4.11",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^6.3.5"
  }
}
